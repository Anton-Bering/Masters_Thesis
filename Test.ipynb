   "source": [
    "from sympy import symbols, simplify\n",
    "\n",
    "# Declare symbols\n",
    "X, Y, Z = symbols('X Y Z')\n",
    "C = symbols('C')\n",
    "\n",
    "\n",
    "\n",
    "# Define T differently\n",
    "T = X**2 + Y**2 + Z**2\n",
    "\n",
    "# Simplify T by substituting C\n",
    "T_simplified = T.subs(X**2 + Y**2, C)\n",
    "\n",
    "# Display the simplified version of T\n",
    "display(T_simplified)\n"
   ]
